{
  "compilerOptions": {
    "target": "ES2020", 
    "module": "CommonJS", 
    "outDir": "dist", 
    "rootDir": ".", 
    "moduleResolution": "node", 
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "strict": true, 
    "resolveJsonModule": true, 
    "types": ["node"]
  }, 
  "include": [
    "src/**/*.ts", 
    "prisma/**/*.ts"
  ], 
  "exclude": ["node_modules", "dist", "prisma/**/*.ts"]
}